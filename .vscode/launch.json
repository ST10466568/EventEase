{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch .NET Core",
        "type": "coreclr",
        "request": "launch",
    "preLaunchTask": "build",
    "program": "${workspaceFolder}/bin/Debug/net9.0/VenueBooking.dll",
    "args": [],
    "cwd": "${workspaceFolder}",
    "stopAtEntry": false,
    "launchBrowser": true,
    "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    },
    "serverReadyAction": {
      "action": "openExternally",
      "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
    },
    "sourceFileMap": {
      "/Users": "${workspaceFolder}"
    }
      }
    ]
  }
  